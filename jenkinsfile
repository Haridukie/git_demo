pipeline {
    agent {
        docker { image 'hariharan1520/getting-started' }
    }
    stages {
        stage('Test') {
            steps {
                sh 'node --version'
            }
        }

        stage('build'){
           agent {
           docker { image 'python:3-slimbuster' }
            }   
        
          steps {
            sh 'python -V'
          }
    }
    }
       
} 