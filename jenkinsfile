pipeline {
    agent {
        docker { image 'hariharan1520/getting-started' }
    }
    stages {
        stage('Test') {
            steps {
                sh 'node --version'
            }
        }
    }
    agent {
      docker {image 'python:3-slimbuster'}
      }
        stage('build'){
        steps 'sh python -V'
    }
} 